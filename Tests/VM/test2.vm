START

// --- Declaração de variáveis (inicialização com 0) ---
PUSHI 0
STOREG 0    // primeiro número
PUSHI 0
STOREG 1    // segundo número
PUSHI 0
STOREG 2    // terceiro número
PUSHI 0
STOREG 3    // variável para o maior número

// --- Programa real começa aqui ---

PUSHS "Introduza o primeiro número: "
WRITES
READ
ATOI
STOREG 0

PUSHS "Introduza o segundo número: "
WRITES
READ
ATOI
STOREG 1

PUSHS "Introduza o terceiro número: "
WRITES
READ
ATOI
STOREG 2

// Comparações para encontrar o maior
PUSHG 0
PUSHG 1
SUP
JZ ELSE1

  PUSHG 0
  PUSHG 2
  SUP
  JZ USENUM3
    PUSHG 0
    JUMP STORE1
  USENUM3:
    PUSHG 2
  STORE1:
  STOREG 3
  JUMP ENDIF

ELSE1:
  PUSHG 1
  PUSHG 2
  SUP
  JZ USENUM3B
    PUSHG 1
    JUMP STORE2
  USENUM3B:
    PUSHG 2
  STORE2:
  STOREG 3

ENDIF:

PUSHS "O maior é: "
WRITES
PUSHG 3
WRITEI
WRITELN

STOP
